<?xml version="1.0" encoding="utf-8" ?>
<!--复制此文档各种Ioc定义方式到需要的地方-->
<objects xmlns="http://www.springframework.net">
  
  <object name="ftpClientForm" type="FtpClient.FtpClientForm, FtpClient">
    <property name="_ftpSettings" ref="_ftpSettings"/>
    <property name="_watchDogForm" ref="_watchDogForm"/>
    <property name="_uploadImageQueue" ref="_uploadImageQueue"/>
  </object>

  <object name="_watchDogForm" type="FtpClient.WatchDogForm, FtpClient">
    <property name="_uploadImageQueue" ref="_uploadImageQueue"/>
  </object>

  <object name="loginForm" type="FtpClient.Login, FtpClient">
    <property name="_ftpSettings" ref="_ftpSettings"/>
  </object>
</objects>
